{
  "name": "fond-of-spryker/checkout-page",
  "description": "Remove customer registration from checkout (accountless checkout)",
  "license": "MIT",
  "authors": [
    {
      "name": "Pascal Fischer",
      "email": "pascal.fischer@fondof.de"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "spryker/glossary": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/zed-request": "^3.0.0",
    "spryker/customer": "^7.10.2",
    "spryker-shop/checkout-page": "^1.2.0",
    "spryker-shop/customer-page": "^1.2.0",
    "fond-of-spryker/customer-page": "1.0.1-RC1",
    "fond-of-spryker/customer": "1.0.1-RC1",
    "fond-of-spryker/shipment": "1.0.0-RC1",
    "spryker/shipment": "^6.5.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.11",
    "codeception/codeception": "^2.3",
    "mikey179/vfsStream": "^1.6",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "php-coveralls/php-coveralls": "^2.0",
    "spryker/kernel": "^3.0.0",
    "spryker/application": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "tests/Generated/"
    }
